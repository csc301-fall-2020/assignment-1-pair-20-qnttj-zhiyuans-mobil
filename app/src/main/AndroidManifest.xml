<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.checkoutmachine">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.AddtoStoreActivity"></activity>
        <activity android:name=".Activity.CheckoutActivity"></activity>
        <activity android:name=".Activity.deleteItemActivity"></activity>
        <activity android:name=".Activity.PayActivity"></activity>
        <activity android:name=".Activity.MenuActivity"></activity>>
    </application>
</manifest>